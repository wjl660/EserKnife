<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="entity">
    <!-- 定义基本数据类型Mapping -->
    <typeAlias alias="int" type="java.lang.Integer" />
    <typeAlias alias="float" type="java.lang.Float" />
    <typeAlias alias="double" type="java.lang.Double" />
    <typeAlias alias="boolean" type="java.lang.Boolean" />
    <typeAlias alias="string" type="java.lang.String" />
    <typeAlias alias="date" type="java.util.Date" />
    <typeAlias alias="map" type="java.util.Map"/>
    <typeAlias alias="long" type="java.lang.Long" />
    <typeAlias alias="short" type="java.lang.Short" />
    <typeAlias alias="timestamp" type="java.sql.Timestamp" />
    <typeAlias alias="hashmap" type="java.util.HashMap" />
    <typeAlias alias="nodeTransportStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeTransportStatInfo"/>
    <typeAlias alias="nodeThreadPoolStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeThreadPoolStatInfo"/>
    <typeAlias alias="nodeOSStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeOSStatInfo"/>
    <typeAlias alias="nodeJVMStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeJVMStatInfo"/>
    <typeAlias alias="nodeHttpStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeHttpStatInfo"/>
    <typeAlias alias="nodeCommonStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeCommonStatInfo"/>
    <typeAlias alias="nodeIndiceStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeIndiceStatInfo"/>
    <typeAlias alias="dataSourceInfo" type="com.ucar.eser.core.bean.po.DataSourceInfo"/>
    <typeAlias alias="nodeFsStatInfo" type="com.ucar.eser.core.bean.vo.stat.NodeFsStatInfo"/>
</sqlMap>